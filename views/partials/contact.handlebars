<section id="contact" style="background: url(http://res.cloudinary.com/dymobf5qb/image/upload/v1509098123/high-quality-abstract-hebus-website-background_tsnodd_copy_bq3mpo.jpg)">
	<div class="container">

		<div class="row mar-bot40">
			<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
				<div class="section-header">
					<h2 class="section-heading animated text-center" data-animation="bounceInUp">Get in Contact</h2>
					<p class="text-center">I don't bite, just fill out this form and I will get back to you ASAP :)</p>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
				<h3>email:</h3>
				<p>aolie94@gmail.com</p>
				<h3>git:</h3>
				<p>www.github.com/SwiftMike</p>
				<a target="_blank" href="https://files.acrobat.com/a/preview/e1fdfaf5-c145-402e-bd72-4c1bf9a094ec"><h4>Click here to see my resume</h4></a>
			</div>
			<div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
				<div class="cform" id="contact-form">
					<div id="errormessage"></div>
					<form id="contactForm" action="/send" method="post" class="form-horizontal">
						<div class="form-group">
							<label for="name" class="col-md-3 control-label">Full Name</label>
							<div class="col-md-6">
								<input placeholder="Your name" type="text" class="form-control" id="name" name="name" />
							</div>
						</div>
						<div class="form-group">
							<label for="email" class="col-md-3 control-label">Email</label>
							<div class="col-md-6">
								<input type="email" id="email" class="form-control" name="email" placeholder="Your Email" />
							</div>
						</div>
						<div class="form-group">
							<label class="col-md-3 control-label">Subject</label>
							<div class="col-md-6">
								<input type="text" class="form-control" name="subject" placeholder="Subject of E-Mail" />
							</div>
						</div>
						<div class="form-group">
							<label class="col-md-3 control-label">Content</label>
							<div class="col-md-6">
								<textarea class="form-control" name="content" rows="5"></textarea>
							</div>
						</div>
						<!-- #messages is where the messages are placed inside -->
						<div class="form-group">
							<div class="col-md-9 col-md-offset-3">
								<div id="messages"></div>
							</div>
						</div>
						<div class="form-group">
							<div class="col-md-9 col-md-offset-3">
								<button type="submit" class="raise btn btn-theme">Send</button>
							</div>
						</div>
					</form>


				</div>
			</div>
			<!-- ./span12 -->
		</div>

	</div>
</section>
